programa
{
	
	funcao inicio()
	{
		real a
		real b
		real c
		real d
		real e1
		real f
		real x
		real y

		escreva ("a: ")
		leia (a)
		escreva ("b: ")
		leia (b)
		escreva ("c: ")
		leia (c)
		escreva ("d: ")
		leia (d)
		escreva ("e: ")
		leia (e1)
		escreva ("f: ")
		leia (f)
		
		x = ((c*e1)-(b*f))/((a*e1)-(b*d))
		y = ((a*f)-(c*d))/((a*e1)-(b*d))
		escreva ("x: " + x)
		escreva ("\ny: " + y)
	}
}
